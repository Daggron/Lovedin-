


<div id="messages">
    <% Object.keys(messages).forEach(function (type) { %>
      <ul class="<%= type %>">
      <% messages[type].forEach(function (message) { %>
        <div class="card-panel pink lighten-2">
        <li><%= message %></li>
       </div>
      <% }) %>
      </ul>
    <% }) %>
</div>